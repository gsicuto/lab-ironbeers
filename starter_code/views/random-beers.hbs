
<div>
  <img src="{{beer.image_url}}" alt="{{beer.name}}">
  <div>
    <h2>{{beer.name}}</h2>
    <h3>{{beer.tagline}}</h3>
    <p>{{beer.description}}</p>
    <h3>Food Pairing</h3>
    <ul>
      {{#each beer.food_pairing}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
    <b>Brewer Tips: </b> <p>{{beer.brewers_tips}}</p>
  </div>
</div>